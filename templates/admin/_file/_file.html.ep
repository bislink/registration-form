% layout 'dashboard';
% title ' File ';
% if ( session->{user}) {
  <article><h2><%= $name %> </h2>
    <%== $out %>
    <footer>
      <h3><%= session->{user} %></h3>
    </footer>
  </article>
  <article class="alert alert-default"><h3>Error</h3>
    <%= $error %>
  </article>

% } else {
  <article><h2>Session Expired</h2>
    <a href="<%= url_for '/' %>" title="Session expired">Login Again</a>
  </article>
% }
